{
  "name": "backblaze-b2",
  "version": "1.6.0",
  "description": "Node.js Library for the Backblaze B2 Storage Service",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run lint-all && npm run test-unit",
    "test-unit": "mocha -r esm \"test/**/*.spec.js\"",
    "lint-lib": "eslint \"lib/**/*.js\" --global Promise",
    "lint-test": "eslint \"test/**/*.js\" --global describe,it,beforeEach,afterEach,Promise",
    "lint-all": "npm run lint-lib && npm run lint-test",
    "build": "npm run-script clean && npx babel ./lib --out-dir ./dist",
    "watch": "watch 'npm run-script build' ./lib",
    "clean": "rm -rf ./dist && mkdir ./dist"
  },
  "keywords": [
    "backblaze",
    "b2",
    "storage"
  ],
  "homepage": "https://github.com/yakovkhalinsky/backblaze-b2",
  "author": {
    "name": "Yakov Khalinsky",
    "email": "yakov@therocketsurgeon.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yakovkhalinsky/backblaze-b2.git"
  },
  "bugs": {
    "url": "https://github.com/yakovkhalinsky/backblaze-b2/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.13",
    "@babel/core": "^7.13",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.23.0",
    "eslint-plugin-jsdoc": "^32.3.0",
    "esm": "^3.2.25",
    "mocha": "^8.3.2",
    "nock": "^13.0.11"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "debug": "^2.6.9"
  },
  "engines": {
    "node": ">=12.0"
  }
}
